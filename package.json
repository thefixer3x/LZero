{
  "name": "vortexai-l0",
  "version": "1.2.2",
  "type": "module",
  "description": "VortexAI L0 - Universal Work Orchestrator CLI",
  "main": "./dist/node/index.js",
  "types": "./dist/browser/index.d.ts",
  "module": "./dist/browser/index.js",
  "bin": {
    "vortex": "dist/node/cli.js",
    "vortexai": "dist/node/cli.js",
    "l0": "dist/node/cli.js",
    "vxai": "dist/node/cli.js",
    "lzero": "dist/node/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js",
      "require": "./dist/node/index.js",
      "default": "./dist/browser/index.js"
    },
    "./cli": "./dist/node/cli.js",
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./orchestrator": {
      "types": "./dist/browser/orchestrator.d.ts",
      "import": "./dist/browser/orchestrator.js",
      "require": "./dist/node/orchestrator.js",
      "default": "./dist/browser/orchestrator.js"
    },
    "./plugins": {
      "types": "./dist/browser/plugins.d.ts",
      "import": "./dist/browser/plugins.js",
      "require": "./dist/node/plugins.js",
      "default": "./dist/browser/plugins.js"
    },
    "./memory-plugin": {
      "types": "./dist/browser/memory-plugin.d.ts",
      "import": "./dist/browser/memory-plugin.js",
      "require": "./dist/node/memory-plugin.js",
      "default": "./dist/browser/memory-plugin.js"
    },
    "./package.json": "./package.json"
  },
  "browser": {
    "./dist/node/index.js": "./dist/browser/index.js",
    "./dist/node/orchestrator.js": "./dist/browser/orchestrator.js",
    "./dist/node/plugins.js": "./dist/browser/plugins.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:node": "tsc -p tsconfig.node.json",
    "dev": "tsc --watch",
    "start": "node dist/node/cli.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "postbuild": "cp package.json dist/",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Linting not configured yet\""
  },
  "keywords": [
    "ai",
    "orchestrator",
    "automation",
    "social-media",
    "content-creation",
    "workflow",
    "vortex",
    "l0"
  ],
  "author": "VortexAI Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.6.2",
    "boxen": "^7.1.1",
    "clipboardy": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thefixer3x/LZero.git"
  },
  "homepage": "https://vortexai.com/l0",
  "engines": {
    "node": ">=18.0.0"
  }
}
